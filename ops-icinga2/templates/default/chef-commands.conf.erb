object CheckCommand "check_elasticsearch" {
  import "plugin-check-command"
  command = [ PluginDir + "/check_elasticsearch" ]
  arguments = {
    "-H" = "$address$"
 }
}

object CheckCommand "check_dynamodb" {
  import "plugin-check-command"
  command = [ PluginDir + "/check_dynamodb.py" ]
  arguments = {
    "-f" = "$fabric$"
    "-m" = "$metric$"
 }
}
