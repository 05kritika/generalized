{
  "variables": {
          "aws_access_key":"",
          "aws_secret_key":"",
          "aws_access_token":"",
          "ftp_password":""
  },
  "builders": [{
          "type": "amazon-ebs",
          "access_key": "{{user `aws_access_key`}}",
          "secret_key": "{{user `aws_secret_key`}}",
          "token": "{{user `aws_access_token`}}",
          "region": "us-west-2",
          "instance_type": "t2.micro",
          "ssh_username": "ec2-user",
          "source_ami": "ami-0e8c04af2729ff1bb",
          "ami_name": "ami-onetick {{timestamp}}"
  }],
  "provisioners": [
    {
      "type": "shell",
      "environment_vars": [
        "ftp_password={{user `ftp_password`}}"
      ],
      "scripts": [
        "scripts/setup.sh"
      ]
    }
  ]
}